<% include ../partials/header.ejs %>

    <!-- Page Content -->
    <div class="container">

        <!-- Portfolio Item Heading -->
        <div class="row">
            <div class="col-lg-12">
                <h1 class="page-header">Drew Before
                    <small>Information Technology Engineer</small>
                </h1>
            </div>
        </div>

        <!-- Related Projects Row -->
        <div class="row">

            <div class="col-lg-12">
                <h3 class="page-header">My Projects</h3>
            </div>
            <% projects.forEach(function(project){ %>
                <div class="col-sm-3 col-xs-6">
                    <a href="/projects/<%= project.title %>">
                        <div class="project">
                            <div class="overlay">
                                <h3 class="text-center"><%= project.title %></h3>
                            </div>
                                <img class="img-responsive portfolio-item" src="<%= project.thumbnail %>" alt=""></img>     
                        </div>
                    </a>
                </div>     
            <% }) %> 
        </div>


<% include ../partials/footer.ejs %>